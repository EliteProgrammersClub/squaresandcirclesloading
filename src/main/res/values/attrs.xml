<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SquaresAndCirclesLoadingView">
        <attr name="circlesColor" format="color" />
        <attr name="animationDuration" format="integer" />
        <attr name="backgroundColor" format="color" />

        <attr name="interpolator" format="enum">
            <enum name="fastOutSlowIn" value="0"/>
            <enum name="anticipate_overshoot" value="1"/>
            <enum name="overshoot" value="2"/>
            <enum name="accelerateDecelerate" value="3"/>
            <enum name="decelerate" value="4"/>
            <enum name="linearOutSlowIn" value="5"/>
        </attr>
    </declare-styleable>
</resources>